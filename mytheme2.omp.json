{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "root",
					"style": "plain",
					"foreground": "#ffe9aa",
					"properties": {
						"prefix": ""
					}
				},
				{
					"type": "path",
					"style": "plain",
					"foreground": "#ff73dc",
					"properties": {
						"folder_separator_icon": "/",
						"style": "letter",
						"prefix": ""
					}
				},
				{
					"type": "git",
					"style": "plain",
					"foreground": "#95ffa4",
					"properties": {
						"display_stash_count": true,
						"display_upstream_icon": true,
						"display_status": true,
						"status_colors_enabled": true,
						"color_background": false,
						"local_changes_color": "#95ffa4",
						"working_color": "#E84855",
						"staging_color": "#b004a2",
						"ahead_color": "#2EC4B6",
						"behind_color": "#8A4FFF",
						"prefix": ""
					}
				},
				{
					"type": "executiontime",
					"style": "powerline",
					"foreground": "#7cc1ee",
					"properties": {
						"always_enabled": true,
						"style": "roundrock",
						"prefix": ""
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"type": "time",
					"style": "plain",
					"powerline_symbol": "\uE0B0",
					"foreground": "#95ffa4",
					"properties": {
						"prefix": ""
					}
				},
				{
					"type": "text",
					"style": "plain",
					"foreground": "#fbff00",
					"properties": {
						"prefix": "",
						"text": "\u276F"
					}
				}
			]
		}
	],
	"final_space": false
}
