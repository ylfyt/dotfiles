{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "path",
					"style": "diamond",
					"powerline_symbol": "\uE0B0",
					"leading_diamond": "",
					"foreground": "#80e1ff",
					"background": "#18354c",
					"properties": {
						"prefix": "",
						"folder_icon": "\uF115",
						"folder_separator_icon": "/",
						"style": "letter",
						"postfix": "<#7a7a7a> \ue0b1</>"
					}
				},
				{
					"type": "git",
					"style": "powerline",
					"powerline_symbol": "",
					"foreground": "#ffc107",
					"background": "#18354c",
					"properties": {
						"display_stash_count": true,
						"display_upstream_icon": true,
						"display_status": true,
						"status_colors_enabled": true,
						"color_background": false,
						"working_color": "#E84855",
						"staging_color": "#95ffa4",
						"ahead_color": "#2EC4B6",
						"behind_color": "#8A4FFF",
						"postfix": "<#7a7a7a> \ue0b1</>",
						"prefix": ""
					}
				},
				{
					"type": "python",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"foreground": "#00ffea",
					"background": "#18354c",
					"properties": {
						"prefix": "",
						"postfix": "<#7a7a7a> \ue0b1</>"
					}
				},
				{
					"type": "executiontime",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"foreground": "#ff73dc",
					"background": "#18354c",
					"properties": {
						"always_enabled": true,
						"style": "roundrock",
						"prefix": ""
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"type": "root",
					"style": "plain",
					"foreground": "#ffe9aa",
					"properties": {
						"postfix": ""
					}
				},
				{
					"type": "time",
					"style": "plain",
					"powerline_symbol": "\uE0B0",
					"foreground": "#95ffa4",
					"properties": {
						"prefix": " ",
						"time_format": "15:04"
					}
				},
				{
					"type": "exit",
					"style": "plain",
					"foreground": "#fbff00",
					"properties": {
						"display_exit_code": false,
						"prefix": "\u276F",
						"always_enabled": true,
						"error_color": "#ef5350"
					}
				}
			]
		}
	],
	"final_space": false
}
