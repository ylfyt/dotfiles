{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "root",
					"style": "powerline",
					"foreground": "#100e23",
					"background": "#ffe9aa"
				},
				{
					"type": "path",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"foreground": "#100e23",
					"background": "#ff73dc",
					"properties": {
						"folder_icon": "\uF115",
						"folder_separator_icon": "/",
						"style": "letter"
					}
				},
				{
					"type": "git",
					"style": "powerline",
					"powerline_symbol": "î‚°",
					"foreground": "#000000",
					"background": "#95ffa4",
					"properties": {
						"display_stash_count": true,
						"display_upstream_icon": true,
						"display_status": true,
						"status_colors_enabled": true,
						"color_background": false,
						"local_changes_color": "#000000",
						"working_color": "#E84855",
						"staging_color": "#2FDA4E",
						"ahead_color": "#2EC4B6",
						"behind_color": "#8A4FFF"
					}
				},
				{
					"type": "executiontime",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"foreground": "#100e23",
					"background": "#7cc1ee",
					"properties": {
						"always_enabled": true,
						"style": "roundrock"
					}
				},
				{
					"type": "python",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"foreground": "#100e23",
					"background": "#906cff",
					"properties": {
						"prefix": " \uE235 "
					}
				},
				{
					"type": "exit",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"foreground": "#ffffff",
					"background": "#ff8080",
					"properties": {
						"prefix": " \uE20F"
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"type": "time",
					"style": "plain",
					"powerline_symbol": "\uE0B0",
					"foreground": "#95ffa4",
					"properties": {
						"prefix": ""
					}
				},
				{
					"type": "text",
					"style": "plain",
					"foreground": "#fbff00",
					"properties": {
						"prefix": "",
						"text": "\u276F"
					}
				}
			]
		}
	],
	"final_space": false
}
